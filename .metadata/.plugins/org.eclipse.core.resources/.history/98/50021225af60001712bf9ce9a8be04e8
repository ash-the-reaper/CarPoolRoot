package ashwin.uomtrust.ac.mu.enums;

import java.util.HashMap;
import java.util.Map;

public enum UserStatus {
	ACTIVE(0),
	DESACTIVE(1);
	
	private int value ;
	private static final Map<Integer, UserStatus> map = new HashMap<>();

	UserStatus(int value) {
		this.value = value;
	}

	public int getValue() {
		return value;
	}

	public void setValue(int value) {
		this.value = value;
	}

	static {
		for (UserStatus item : values()) {
			map.put(item.getValue(), item);
		}
	}

	public static UserStatus valueFor(int ref) {
		return map.get(ref);
	}
}