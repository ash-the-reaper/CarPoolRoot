//var main_url = "http://localhost:8081/api/";
var main_url = "http://localhost/api/";
var request = "request/";

var urlGetAllTripsCreatedToday = main_url + request + "getAllTripsCreatedToday";


var app = angular.module("ngApp", []);
app.controller("ngCtrl", function($scope,$http) {
	
	$http.post(urlGetAllTripsCreatedToday).success(function (response) {
    	$scope.tripsCreatedToday = response;    	
	});

});